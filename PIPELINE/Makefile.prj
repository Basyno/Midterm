#
# Dependency Generation rule for BEH module
#
$(BDW_OBJDIR)/bubblesort.d :  bubblesort.cc
	@if [ ! -d $(BDW_OBJDIR) ]; then mkdir -p $(BDW_OBJDIR); fi
	@echo "Generating dependencies for $< "
	@set -e; $(BDW_CCDEP) -MM -MG $(BDW_CCFLAGS)  bubblesort.cc \
	| sed 's|bubblesort\.o[ :]*|$(BDW_OBJDIR)/bubblesort.o $(BDW_OBJDIR)/bubblesort.d : |' $(BDW_DEP_FILTER)  > $@

$(BDW_WORKLIB)/wrappers/bubblesort_trace.h : $(BDW_WORKLIB)/wrappers/bubblesort_trace.h.updated

$(BDW_WORKLIB)/wrappers/bubblesort_trace.h.updated :  bubblesort.cc $(BDW_LOGOPTIONS_FILE)
	$(STRATUS_HOME)/bin/bdw_tracegen -module bubblesort -vcd 
	@touch $(BDW_WORKLIB)/wrappers/bubblesort_trace.h.updated



$(BDW_WORKLIB)/objs/bubblesort.o:  bubblesort.cc $(BDW_WORKLIB)/wrappers/bubblesort_wrap.cc $(BDW_WORKLIB)/wrappers/bubblesort_wrap.h $(BDW_WORKLIB)/wrappers/bubblesort_cosim.h $(BDW_WORKLIB)/wrappers/bubblesort_trace.h 
	@if [ ! -d $(BDW_OBJDIR) ]; then mkdir -p $(BDW_OBJDIR); fi
	$(BDW_CC)  $(BDW_CCFLAGS)   -o $(BDW_WORKLIB)/objs/bubblesort.o  $(BDW_WORKLIB)/wrappers/bubblesort_wrap.cc

.INTERMEDIATE : bubblesort_wrap.h

bubblesort_wrap.h : $(BDW_WORKLIB)/wrappers/bubblesort_wrap.h

$(BDW_WORKLIB)/wrappers/bubblesort_wrap.cc $(BDW_WORKLIB)/wrappers/bubblesort_wrap.h $(BDW_WORKLIB)/wrappers/bubblesort_cosim.h $(BDW_WORKLIB)/wrappers/bubblesort_cosim.v   : $(BDW_WRAPDIR)/bubblesort.updated

$(BDW_WRAPDIR)/bubblesort.updated :  bubblesort.cc 
	@if [ ! -d $(BDW_WRAPDIR) ]; then mkdir -p $(BDW_WRAPDIR); fi
	$(STRATUS_HOME)/bin/bdw_wrapgen -project project.tcl -module bubblesort "-I$(BDW_WRAPDIR) $(BDW_LIB_DASHI) $(BDW_CCOPTIONS) $(BDW_EXTRA_CCFLAGS)"
	@touch $(BDW_WRAPDIR)/bubblesort.updated
	$(STRATUS_HOME)/bin/bdw_tracegen -module bubblesort -vcd 
	@touch $(BDW_WORKLIB)/wrappers/bubblesort_trace.h.updated
$(BDW_WORKLIB)/wrappers/bubblesort_wrap.d :  bubblesort.cc $(BDW_WORKLIB)/wrappers/bubblesort_wrap.h
	@if [ ! -d $(@D) ]; then mkdir -p $(@D); fi
	@set -e; $(BDW_CCDEP) -MM -MG $(BDW_CCFLAGS)   bubblesort.cc \
		| sed 's|bubblesort\.o[ :]*|$(BDW_WRAPDIR)/bubblesort.updated $(BDW_WORKLIB)/wrappers/bubblesort_wrap.d :  |' $(BDW_DEP_FILTER) > $(BDW_WORKLIB)/wrappers/bubblesort_wrap.d 




